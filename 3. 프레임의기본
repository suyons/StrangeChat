package swing.frame;

import javax.swing.JFrame;

public class MyApp {

	public static void main(String[] args) {
		JFrame f = new JFrame("코스모 프로젝트1");	//프레임 생성자
//		f.setTitle("코스모 프로젝트1");	//프레임 제목 위에처럼 해도되고
									//위에 비우고 이렇게 타이틀 줘도 됨
		f.setSize(400, 500);		//프레임 사이즈
		f.setVisible(true);			//프레임 보이기
		

		
		
		
	}

}
